<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.TeamSceneController">
   <children>
      <HBox id="top-hbox" alignment="CENTER" prefHeight="156.0" prefWidth="600.0" styleClass="hbox">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button id="small-button" fx:id="backButton" mnemonicParsing="false" onAction="#goBack" text="Back" />
               </children>
            </HBox>
            <HBox id="top-middle-hbox" alignment="CENTER" prefHeight="123.0" prefWidth="293.0" styleClass="hbox">
               <children>
                  <Text id="title-text" fx:id="teamName" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text" text="Name of Team" textAlignment="CENTER" wrappingWidth="325.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button id="small-button" fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteTeam" text="Delete Team" textAlignment="CENTER" />
               </children>
            </HBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="271.0" prefWidth="600.0" styleClass="hbox">
         <children>
            <Button id="large-button" fx:id="editTeamButton" mnemonicParsing="false" onAction="#editTeam" style="-fx-font-size: 20;" text="Edit Team" />
            <Button id="large-button" mnemonicParsing="false" onAction="#eventScene" style="-fx-font-size: 20;" text="Track Stats" />
         </children>
      </HBox>
   </children>
</VBox>
